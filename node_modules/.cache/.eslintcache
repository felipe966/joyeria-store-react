d ? { ...x, qty: x.qty + 1, sub_total: (x.qty + 1) * x.price } : x\n        );\n      }\n\n      return [\n        ...state,\n        {\n          ...product,\n          qty: 1,\n          sub_total: product.price\n        },\n      ];\n\n    case \"DELITEM\":\n      const exist1 = state.find((x) => x.id === product.id);\n      if (exist1.qty === 1) {\n        return state.filter((x) => x.id !== exist1.id);\n      } else {\n        return state.map((x) =>\n          x.id === product.id ? { ...x, qty: x.qty - 1, sub_total: (x.qty - 1) * x.price } : x\n        );\n      }\n\n    case \"RMVITEM\":\n      const exist2 = state.find((x) => x.id === product.id);\n      if (exist2.qty) {\n        return state.filter((x) => x.id !== exist2.id);\n      }\n\n    default:\n      return state;\n      \n  }\n};\n\nexport default handleCart;\n","/Users/spike/Dev/UNED/proyecto2/joyeria-store-react/src/components/Login/index.js",[],[],"/Users/spike/Dev/UNED/proyecto2/joyeria-store-react/src/components/Register/index.js",[],[],"/Users/spike/Dev/UNED/proyecto2/joyeria-store-react/src/components/cart/CartProduct.jsx",[],[],"/Users/spike/Dev/UNED/proyecto2/joyeria-store-react/src/redux/action/index.js",[],[],"/Users/spike/Dev/UNED/proyecto2/joyeria-store-react/src/components/About.jsx",["82","83"],[],"/Users/spike/Dev/UNED/proyecto2/joyeria-store-react/src/components/Contact.jsx",["84","85","86"],[],{"ruleId":"87","severity":1,"message":"88","line":26,"column":28,"nodeType":"89","endLine":26,"endColumn":33},{"ruleId":"87","severity":1,"message":"90","line":28,"column":6,"nodeType":"91","endLine":28,"endColumn":8,"suggestions":"92"},{"ruleId":"93","severity":1,"message":"94","line":42,"column":5,"nodeType":"95","messageId":"96","endLine":43,"endColumn":20},{"ruleId":"97","severity":1,"message":"98","line":2,"column":10,"nodeType":"99","messageId":"100","endLine":2,"endColumn":17},{"ruleId":"97","severity":1,"message":"101","line":3,"column":10,"nodeType":"99","messageId":"100","endLine":